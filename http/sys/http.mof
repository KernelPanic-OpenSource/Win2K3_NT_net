]
class HttpCachedSend:HttpRequest
{
    [WmiDataId(1),
     Description("Request Object") : amended,
     PointerType,
     read]
     uint32  RequestObj;
    [WmiDataId(2),
     Description("Site Id") : amended,
     read]
