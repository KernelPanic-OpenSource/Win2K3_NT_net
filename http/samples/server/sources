C_DEFINES=$(C_DEFINES) -DNT -DUNICODE -D_UNICODE

TARGETLIBS=\
    $(SDK_LIB_PATH)\mswsock.lib  \
    $(SDK_LIB_PATH)\ntdll.lib    \
    $(SDK_LIB_PATH)\user32.lib   \
    $(SDK_LIB_PATH)\ws2_32.lib   \
    ..\..\api\$(O)\httpapi.lib   \


SOURCES=                         \
        main.c


!IF "$(NTNOPCH)" == ""
PRECOMPILED_INCLUDE=precomp.h
