    $(SDK_LIB_PATH)\shell32.lib \
    $(PROJECT_ROOT)\mmc\common\$(O)\snapcomn.lib \
    $(SDK_LIB_PATH)\user32.lib \
    $(SDK_LIB_PATH)\uuid.lib \
    $(PROJECT_ROOT)\wins\rpc\$(O)\winsrpc.lib \
    $(SDK_LIB_PATH)\wsock32.lib

INCLUDES=..\common;..\tfscore;..\idl\$(O);     \
         $(DS_INC_PATH); \
         $(SHELL_INC_PATH)

